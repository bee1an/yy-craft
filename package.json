{
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "pnpm -C play dev",
    "doc:dev": "pnpm -C docs dev",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist && rimraf node_modules",
    "publish": "pnpm i --frozen-lockfile && pnpm build && cd dist/yy-ui && npm publish --provenance"
  },
  "dependencies": {
    "@css-render/plugin-bem": "0.15.14",
    "@yy-ui/components": "workspace:*",
    "@yy-ui/composables": "workspace:*",
    "@yy-ui/constants": "workspace:*",
    "@yy-ui/directives": "workspace:*",
    "@yy-ui/theme-chalk": "workspace:*",
    "@yy-ui/utils": "workspace:*",
    "css-render": "0.15.14"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vue/eslint-config-typescript": "14.1.4",
    "commander": "^13.1.0",
    "eslint": "9.16.0",
    "eslint-plugin-vue": "9.32.0",
    "globals": "15.13.0",
    "rimraf": "6.0.1",
    "rollup": "4.34.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-esbuild": "6.2.0",
    "rollup-plugin-visualizer": "5.14.0",
    "shelljs": "0.8.5",
    "typescript": "5.7.2",
    "typescript-eslint": "8.17.0",
    "vue": "3.5.13"
  }
}
