{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "tsx scripts/cleanup.ts -d && rolldown -c rolldown.config.ts && tsx scripts/replace-dts.ts",
    "build:dev": "cross-env NODE_ENV=development pnpm build",
    "build:prod": "cross-env NODE_ENV=production pnpm build",
    "dev": "pnpm -C play dev",
    "doc:dev": "pnpm -C docs dev",
    "doc:build": "pnpm -C docs build",
    "typecheck": "vue-tsc -p tsconfig.json --noEmit",
    "cleanup": "tsx scripts/cleanup.ts -d -n && node -e \"fs.rmSync('node_modules', { recursive: true, force: true })\"",
    "auto:publish": "tsx scripts/publish.ts",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "test": "vitest",
    "create": "tsx scripts/create/create.ts",
    "lint": "eslint  --fix"
  },
  "peerDependencies": {
    "vue": "3.2.25"
  },
  "dependencies": {
    "@css-render/plugin-bem": "0.15.14",
    "@vicons/ionicons5": "0.12.0",
    "css-render": "0.15.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@types/prompts": "^2.4.9",
    "@types/shelljs": "0.8.15",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.2",
    "@vue/test-utils": "2.4.6",
    "commander": "13.1.0",
    "cross-env": "7.0.3",
    "cz-git": "1.11.1",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "fast-glob": "3.3.3",
    "fs-extra": "11.3.0",
    "globals": "15.13.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "prompts": "2.4.2",
    "rolldown": "1.0.0-beta.28",
    "rollup-plugin-copy": "3.5.0",
    "shelljs": "0.8.5",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "unplugin-vue": "6.0.0",
    "unplugin-vue-jsx": "0.6.1",
    "vite-plugin-dts": "4.5.0",
    "vitest": "3.0.9",
    "vue": "3.2.25",
    "vue-macros": "3.0.0-beta.17",
    "vue-tsc": "2.1.10"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
